{
    "build": {
      "dockerfile": "./Dockerfile"
    },
    // "postStartCommand": "make install-dependencies",
    "customizations": {
      "vscode": {
        "settings": {
          "editor.insertSpaces": true,
          "editor.formatOnType": true
        },
        "extensions": [
          "eamodio.gitlens",
          "EditorConfig.EditorConfig",
          "EdwinKofler.vscode-assorted-languages",
          "ms-python.python",
          "IBM.output-colorizer",
          "mechatroner.rainbow-csv",
          "ms-python.debugpy",
          "HoangKimLai.ipython",
          "ms-python.vscode-pylance",
          "ms-toolsai.datawrangler",
          "ms-toolsai.jupyter",
          "ms-toolsai.vscode-jupyter-powertoys",
          "ms-vscode.sublime-keybindings",
          "streetsidesoftware.code-spell-checker",
          "streetsidesoftware.code-spell-checker-portuguese-brazilian",
          "wmaurer.change-case",
          "yo1dog.cursor-align"
        ]
      }
    },
    "features": {
      "ghcr.io/devcontainers/features/docker-in-docker:2": {},
      "ghcr.io/devcontainers/features/github-cli:1": {},
      "ghcr.io/devcontainers/features/python:1": {},
      "ghcr.io/devcontainers/features/node:1": {},
      "ghcr.io/devcontainers/features/sshd:1": {},
    //   "ghcr.io/devcontainers/features/dotnet:2": {},
      // "ghcr.io/devcontainers/features/azure-cli:1": {},
    //   "ghcr.io/devcontainers/features/java:1": {},
    //   "ghcr.io/jlaundry/devcontainer-features/mssql-odbc-driver:1": {},
      "ghcr.io/stuartleeks/dev-container-features/shell-history:0": {},
      // "ghcr.io/devcontainers/features/kubectl-helm-minikube:1": {},
    //   "ghcr.io/NordcomInc/devcontainer-features/android-sdk:1": {},
      "ghcr.io/devcontainers/features/desktop-lite:1": {},
      "ghcr.io/devcontainers/features/common-utils:2": {
        "installZsh": true,
        "installOhMyZsh": true,
        "installOhMyZshConfig": true,
        "configureZshAsDefaultShell": true,
        "upgradePackages": true,
        "nonFreePackages": true
      }
    },
    "overrideFeatureInstallOrder": [
      "ghcr.io/devcontainers/features/common-utils",
      "ghcr.io/devcontainers/features/sshd",
    //   "ghcr.io/devcontainers/features/dotnet",
    //   "ghcr.io/devcontainers/features/java",
      "ghcr.io/devcontainers/features/docker-in-docker",
      "ghcr.io/devcontainers/features/python",
      "ghcr.io/devcontainers/features/node",
    //   "ghcr.io/jlaundry/devcontainer-features/mssql-odbc-driver",
      "ghcr.io/stuartleeks/dev-container-features/shell-history",
      //   "ghcr.io/NordcomInc/devcontainer-features/android-sdk",
      "ghcr.io/devcontainers/features/desktop-lite"
    ]
  }